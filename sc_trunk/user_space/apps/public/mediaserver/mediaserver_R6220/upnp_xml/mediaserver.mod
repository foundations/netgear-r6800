<?xml version="1.0" encoding="utf-8"?>
<root xmlns="urn:schemas-upnp-org:device-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<device>
		<deviceType>urn:schemas-upnp-org:device:MediaServer:1</deviceType>
		<friendlyName>@FRIENDLY_NAME#</friendlyName>
		<manufacturer>@MANUFACTURER#</manufacturer>
		<manufacturerURL>@MANUFACTURER_URL#</manufacturerURL>
		<modelDescription>@MODEL_DESCRIPTION#</modelDescription>
		<modelName>@MODEL_NAME#</modelName>
		<modelNumber>@MODEL_NUMBER#</modelNumber>
		<serialNumber>@SERIAL_NUMBER#</serialNumber>
		<UDN>uuid:@UUID#</UDN>
		<iconList>
			<icon>
				<mimetype>image/jpeg</mimetype>
				<height>48</height>
				<width>48</width>
				<depth>24</depth>
				<url>/images/upnpav-48.jpg</url>
			</icon>
			<icon>
				<mimetype>image/jpeg</mimetype>
				<height>120</height>
				<width>120</width>
				<depth>24</depth>
				<url>/images/upnpav-120.jpg</url>
			</icon>
			<icon>
				<mimetype>image/png</mimetype>
				<height>48</height>
				<width>48</width>
				<depth>24</depth>
				<url>/images/upnpav-48.png</url>
			</icon>
			<icon>
				<mimetype>image/png</mimetype>
				<height>120</height>
				<width>120</width>
				<depth>24</depth>
				<url>/images/upnpav-120.png</url>
			</icon>
		</iconList>
		<serviceList>
			<service>
				<serviceType>urn:schemas-upnp-org:service:ConnectionManager:1</serviceType>
				<serviceId>urn:upnp-org:serviceId:ConnectionManager</serviceId>
				<SCPDURL>/connection.xml</SCPDURL>
				<controlURL>/ConnectionManager/Control</controlURL>
				<eventSubURL>/ConnectionManager/Event</eventSubURL>
			</service>
			<service>
				<serviceType>urn:schemas-upnp-org:service:ContentDirectory:1</serviceType>
				<serviceId>urn:upnp-org:serviceId:ContentDirectory</serviceId>
				<SCPDURL>/content.xml</SCPDURL>
				<controlURL>/ContentDirectory/Control</controlURL>
				<eventSubURL>/ContentDirectory/Event</eventSubURL>
			</service>
		</serviceList>
		<presentationURL>http://@IPADDR#:@DEVICE_PORT#/</presentationURL>
		 <dlna:X_DLNADOC xmlns:dlna="urn:schemas-dlna-org:device-1-0">DMS-1.50</dlna:X_DLNADOC> 
	</device>
</root>
